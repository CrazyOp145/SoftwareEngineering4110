<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Profiles.Login.LoginController">
   <children>
      <PasswordField fx:id="passwordTextField" layoutX="225.0" layoutY="200.0" promptText="Password" />
      <TextField fx:id="userIDTextField" layoutX="225.0" layoutY="175.0" promptText="User ID" />
      <Text layoutX="129.0" layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0" text="WSU INC. Wholesale Food Distribution">
         <font>
            <Font size="20.0" />
         </font>
      </Text>
      <Button fx:id="loginButton" alignment="CENTER" layoutX="264.0" layoutY="237.0" mnemonicParsing="false" onAction="#loginButtonOnAction" prefHeight="25.0" prefWidth="72.0" text="Login" />
      <Button fx:id="exitButton" alignment="CENTER" layoutX="264.0" layoutY="268.0" mnemonicParsing="false" onAction="#exitButtonOnAction" prefHeight="25.0" prefWidth="72.0" text="Exit" />
      <Text fx:id="loginMessageLabel" layoutX="182.0" layoutY="159.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please Enter In User ID and Password" textAlignment="CENTER" wrappingWidth="235.0" />
   </children>
</AnchorPane>
