<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DeleteItemController">
    <children>
        <Label layoutX="57.0" layoutY="42.0" text="Delete item :">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="deleteItemBar" layoutX="194.0" layoutY="43.0" prefHeight="25.0" prefWidth="206.0" />
        <TableView fx:id="tableView" layoutX="31.0" layoutY="94.0" prefHeight="269.0" prefWidth="542.0">
            <columns>
                <TableColumn fx:id="itemId" prefWidth="75.0" text="Item ID" />
                <TableColumn fx:id="itemName" prefWidth="74.0" text="Name" />
                <TableColumn fx:id="quantity" prefWidth="76.0" text="Quantity" />
                <TableColumn fx:id="sellingPrice" prefWidth="75.0" text="Selling Price" />
                <TableColumn fx:id="purchasePrice" prefWidth="92.0" text="Purchase Price" />
                <TableColumn fx:id="expireDate" prefWidth="149.0" text="Expiring Date" />
            </columns>
        </TableView>
        <Button fx:id="goBack" layoutX="485.0" layoutY="367.0" mnemonicParsing="false" onAction="#switchToUserMenu" text="Back to menu" />
      <Button fx:id="deleteButton" layoutX="414.0" layoutY="43.0" mnemonicParsing="false" onAction="#deleteProfile" text="Delete" />
    </children>
</AnchorPane>
