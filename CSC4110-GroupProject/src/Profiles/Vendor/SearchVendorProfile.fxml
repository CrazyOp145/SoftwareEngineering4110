<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Profiles.Vendor.SearchVendorProfileController"
            prefHeight="400.0" prefWidth="700.0">
    <children>
        <Label layoutX="57.0" layoutY="42.0" text="Search Vendor:">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <TextField fx:id="searchVendor" layoutX="194.0" layoutY="43.0" prefHeight="25.0" prefWidth="357.0" />
        <TableView fx:id="tableView" layoutX="31.0" layoutY="94.0" prefHeight="269.0" prefWidth="542.0">
            <columns>
                <TableColumn fx:id="vendID" prefWidth="75.0" text="Customer ID" />
                <TableColumn fx:id="vendorName" prefWidth="74.0" text="Company Name" />
                <TableColumn fx:id="street" prefWidth="76.0" text="Street Address" />
                <TableColumn fx:id="city" prefWidth="75.0" text="City" />
                <TableColumn fx:id="state" prefWidth="92.0" text="State" />
                <TableColumn fx:id="phone" prefWidth="149.0" text="Phone" />
                <TableColumn fx:id="balance" prefWidth="149.0" text="Balance" />
                <TableColumn fx:id="lastPaidAmount" prefWidth="149.0" text="Last Paid Amount" />
                <TableColumn fx:id="lastOrderDate" prefWidth="149.0" text="Last Order Date" />
                <TableColumn fx:id="seasDiscDate" prefWidth="149.0" text="Seasonal Discount"/>
            </columns>
        </TableView>
        <Button fx:id="goBack" layoutX="485.0" layoutY="367.0" mnemonicParsing="false" onAction="#switchToUserMenuScene" text="Back to menu" />
    </children>
</AnchorPane>
